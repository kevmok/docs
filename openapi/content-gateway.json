    "/files/{id}/download": {
      "get": {
        "summary": "Download file",
        "description": "Retrieves and downloads the entire media file based on the specified file ID. A checksum is provided to validate the integrity of the downloaded file.\n",
        "parameters": [
          {
            "in": "path",
            "name": "id",
            "required": true,
            "schema": {
              "type": "string",
              "format": "uuid",
              "description": "Unique identifier for the file to download."
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successfully retrieved the entire file. The response body contains the file data.",
            "headers": {
              "Content-Type": {
                "schema": {
                  "type": "string",
                  "description": "The MIME type of the returned content.",
                  "example": "application/pdf"
                }
              },
              "Content-Length": {
                "schema": {
                  "type": "integer",
                  "description": "The length of the returned content in bytes."
                }
              },
              "Checksum-SHA256": {
                "schema": {
                  "type": "string",
                  "description": "The SHA-256 checksum of the entire file, allowing clients to verify file integrity.",
                  "example": "d7a8fbb307d4c672a8f3ddf3dbf832b734482ea9c18496351ed3cc631d446b9e"
                }
              }
            },
            "content": {
              "application/octet-stream": {
                "schema": {
                  "type": "string",
                  "format": "binary"
                }
              }
            }
          },
          "400": {
            "description": "Bad Request - The request could not be understood by the server due to malformed syntax.",
