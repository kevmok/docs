                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          },
          "503": {
            "description": "Service Unavailable - The server is currently unable to handle the request.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "custom_attributes": {
        "type": "object",
        "additionalProperties": {
          "type": "string"
        },
        "description": "A map of string to string for representing custom attributes."
      },
      "status_enum": {
        "type": "string",
        "enum": [
          "active",
          "deleted"
        ]
      },
      "node_reference": {
        "type": "object",
        "required": [
          "id",
          "name"
        ],
        "properties": {
          "id": {
            "type": "string",
            "format": "uuid",
            "example": "parent123"
          },
          "name": {
            "type": "string",
            "example": "Parent Node Name"
          },
          "path": {
            "type": "string",
            "example": "/reports/2023"
          }
        }
      },
      "Node": {
        "type": "object",
        "required": [
          "id",
          "name",
          "external_url",
          "last_modified_datetime"
        ],
        "properties": {
          "id": {
            "type": "string",
            "format": "uuid",
            "example": "12345",
            "description": "Unique id for each file"
          },
          "name": {
            "type": "string",
            "example": "Document or Folder Name",
            "description": "User friendly name for the file"
          },
          "status": {
            "$ref": "#/components/schemas/status_enum"
          },
          "custom_attributes": {
            "$ref": "#/components/schemas/custom_attributes"
          },
          "last_modified_datetime": {
            "type": "string",
            "format": "date-time",
            "example": "2023-04-17T12:34:56Z",
            "description": "Used to efficiently ingest content only updated since last ingestion"
          },
          "last_modified_by": {
            "type": "string",
            "example": "john_doe@abc.com"
          },
          "created_datetime": {
            "type": "string",
            "format": "date-time",
            "example": "2023-01-01T00:00:00Z"
          },
          "created_by": {
            "type": "string",
            "example": "jane_doe@abc.com"
          },
          "external_url": {
            "type": "string",
            "example": "https://intranet.example.com/files/AnnualReport2024.pdf",
            "description": "External URL to redirect users"
          },
          "metadata_url": {
            "type": "string",
            "example": "https://intranet.example.com/content/files/<file_id>",
            "description": "Used to retrieve metadata about individual files"
          },
          "content": {
            "type": "object",
            "required": [
              "mime_type"
            ],
            "properties": {
              "download_path": {
                "type": "string",
                "example": "12345/download",
                "description": "URL for the download request, most likely `/files/{id}/download`. *Either `download_path` OR `body` MUST be provided.*"
              },
              "body": {
                "type": "string",
                "description": "The HTML representation of the object. If `mime_type` is `text/html` and `download_path` is not provided, this will be ingested as the content of the document. *Either `download_path` OR `body` MUST be provided.*"
              },
              "mime_type": {
                "type": "string",
                "example": "application/pdf",
                "description": "Used to specify type of content. See our supported MIME types here: https://help.moveworks.com/reference/supported-mime-types."
              },
              "size": {
                "type": "integer",
                "format": "int64",
                "example": 102400
              },
              "sha1_hash": {
                "type": "string",
                "example": "2fd4e1c67a2d28fced849ee1bb76e7391b93eb12"
              }
            }
          },
          "parent_info": {
            "$ref": "#/components/schemas/node_reference"
          },
          "children_url": {
            "type": "string",
            "example": "https://content-gateway-example.com/v1/content/files?$filter=(folder eq 12345)"
          }
        }
      },
      "ODataContentResponse": {
        "type": "object",
        "additionalProperties": false,
        "properties": {
          "@odata.context": {
            "type": "string",
            "description": "URL to the metadata of the response.",
            "example": "https://content-gateway-example.com/v1/$metadata#Content"
          },
          "value": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Node"
            }
          },
          "@odata.nextLink": {
            "type": "string",
            "description": "URL to fetch the next set of results.",
            "example": "https://content-gateway-example.com/v1/content?$filter=(category in (retail, sales) and file_type in (pdf, docx))&$top=10&$skip=10&$orderby=createdDateTime desc"
          }
        }
      },
      "ODataContentMetadataResponse": {
        "type": "object",
        "additionalProperties": false,
        "properties": {
          "@odata.context": {
            "type": "string",
            "description": "URL to the metadata of the response.",
            "example": "https://content-gateway-example.com/v1/$metadata#Content"
          },
          "value": {
            "$ref": "#/components/schemas/Node"
          }
        }
      },
      "ErrorResponse": {
        "type": "object",
        "required": [
          "error"
        ],
        "properties": {
          "error": {
            "type": "object",
            "required": [
              "code",
              "message"
            ],
            "properties": {
              "code": {
                "type": "string",
                "description": "Standard error code (e.g., UNAUTHORIZED, INVALID_ARGUMENT, NOT_FOUND)."
              },
              "message": {
                "type": "string",
                "description": "Human-readable explanation of the error."
              }
            }
          }
        }
      },
      "UserCollection": {
        "type": "object",
        "properties": {
          "@odata.context": {
            "type": "string"
          },
          "value": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/User"
            }
          },
          "@odata.nextLink": {
            "type": "string"
          }
        }
      },
      "User": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "description": "Unique identifier for the user."
          },
          "primary_email_addr": {
            "type": "string",
            "description": "Primary email address of the user."
          },
          "full_name": {
            "type": "string",
            "description": "Full name of the user."
          },
          "state": {
            "type": "string",
            "description": "State of the user account. Enum: Active | Inactive"
          },
          "last_modified_datetime": {
            "type": "string",
            "description": "Last modification timestamp (ISO 8601)."
          },
          "metadata": {
            "type": "object",
            "description": "Custom key-value metadata for the user."
          }
        }
      },
      "GroupCollection": {
        "type": "object",
        "properties": {
          "@odata.context": {
            "type": "string"
          },
          "value": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Group"
            }
          },
          "@odata.nextLink": {
            "type": "string"
          }
        }
      },
      "Group": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "description": "Unique identifier for the group."
          },
          "name": {
            "type": "string",
            "description": "Name of the group."
          },
          "created_datetime": {
            "type": "string",
            "description": "Timestamp when the group was created."
          },
          "last_modified_datetime": {
            "type": "string",
            "description": "Timestamp when the group was last updated."
          },
          "metadata": {
            "type": "object",
            "description": "Custom key-value metadata for the group."
          }
        }
      },
      "GroupMemberCollection": {
        "type": "object",
        "properties": {
          "@odata.context": {
            "type": "string"
          },
          "value": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/GroupMember"
            }
          },
          "@odata.nextLink": {
            "type": "string"
          }
        }
      },
      "GroupMember": {
        "type": "object",
        "properties": {
          "type": {
            "type": "string",
            "description": "Entity type. Enum: USER | GROUP"
          },
          "id": {
            "type": "string",
            "description": "ID of the member (user or group)."
          },
          "name": {
            "type": "string",
            "description": "Name of the member (optional)."
          }
        }
      },
      "PermissionsMetadata": {
        "type": "object",
        "properties": {
          "@odata.context": {
            "type": "string",
            "description": "OData metadata context URL."
          },
          "model": {
            "type": "string",
            "enum": [
              "resource_permission",
              "hierarchy_permission"
            ],
            "description": "Permission model in use. NOTE: `resource_permissions` is the only supported model at this time."
          }
        }
      },
      "PermissionCollection": {
        "type": "object",
        "properties": {
          "@odata.context": {
            "type": "string",
            "description": "OData metadata context URL."
          },
          "value": {
            "type": "object",
            "properties": {
              "permissions": {
                "type": "array",
                "description": "List of permission mappings for users and groups.",
                "items": {
                  "type": "object",
                  "properties": {
                    "type": {
                      "type": "string",
                      "enum": [
                        "USER",
                        "GROUP"
                      ],
                      "description": "Type of permission entity."
                    },
                    "id": {
                      "type": "string",
                      "description": "Entity ID (user or group). Wildcard (`*`), used in conjunction with `type` `GROUP`, can be used to denote all Users."
                    },
                    "action": {
                      "type": "string",
                      "enum": [
                        "VIEW",
                        "UPDATE"
                      ],
                      "description": "Action permitted on the content."
                    }
                  }
                }
              },
              "permissions_hierarchy": {
                "type": "array",
                "description": "Hierarchy chain of entity IDs (required if permission model = hierarchy_permission).",
                "items": {
                  "type": "string"
                }
              },
              "last_modified_datetime": {
                "type": "string",
                "description": "Timestamp of the last permission update."
              }
            }
          },
          "@odata.nextLink": {
            "type": "string",
            "description": "URL to retrieve the next page of permission results."
          }
        }
      }
    }
  }
}